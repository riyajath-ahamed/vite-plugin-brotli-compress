{
  "name": "vite-plugin-brotli-compress",
  "version": "1.0.3",
  "description": "A Vite plugin to compress build assets using Brotli, built with tsdown.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build && npm run test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riyajath-ahamed/vite-plugin-brotli-compress.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "compress",
    "brotli",
    "react",
    "typescript"
  ],
  "author": "riyajath-ahamed <riyajath.ahamed@gmail.com> (https://github.com/riyajath-ahamed)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/riyajath-ahamed/vite-plugin-brotli-compress/issues"
  },
  "homepage": "https://github.com/riyajath-ahamed/vite-plugin-brotli-compress#readme",
  "peerDependencies": {
    "vite": ">=4.0.0 <8.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.8.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsdown": "^0.15.7",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.52.4"
  }
}
